{
	"name": "UespPatreon",
	"version": "0.1",
	"author": [
		"[https://uesp.net/wiki/User:Daveh Daveh]"
	],
	"descriptionmsg": "uesppatreon-desc",
	"license-name": "MIT",
	"type": "other",
	"AutoloadClasses": {
		"UespPatreonCommon": "UespPatreonCommon.php",
		"UespPatreonHooks": "UespPatreonHooks.php",
		"SpecialUespPatreon": "SpecialUespPatreon.php"
	},
	"Hooks": {
		"UserGetDefaultOptions": [
			"UespPatreonHooks::userGetDefaultOptions"
		],
		"GetPreferences": [
			"UespPatreonHooks::getPreferences"
		],
		"LoadExtensionSchemaUpdates": [
			"UespPatreonHooks::onLoadExtensionSchemaUpdates"
		]
	},
	"SpecialPages": {
		"UespPatreon": "SpecialUespPatreon"
	},
	"MessagesDirs": {
		"UespPatreon": [
			"i18n"
		]
	},
	"GroupPermissions": {
		"*" : {
			"patreon-link" : true,
			"patreon-view" : false,
			"patreon-edit" : false,
			"patreon-shipment" : false
		},
		"sysop" : {
			"patreon-link" : true,
			"patreon-view" : true,
			"patreon-edit" : false,
			"patreon-shipment" : false
		},
		"patreon-viewer" : {
			"patreon-link" : true,
			"patreon-view" : true,
			"patreon-edit" : false,
			"patreon-shipment" : false
		},
		"patreon-editor" : {
			"patreon-link" : true,
			"patreon-view" : true,
			"patreon-edit" : true,
			"patreon-shipment" : true
		}
	},
	"ResourceModules" : {
		"ext.UespPatreon.modules": {
			"localBasePath": "modules",
			"remoteExtPath": "UespPatreon/modules",
			"styles": [
				"uespPatreon.css"
			],
			"scripts": [
				"uespPatreon-Countries.js",
				"uespPatreon-Deminis.js",
				"uespPatreon.js"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"manifest_version": 1
}